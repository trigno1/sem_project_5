generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model NFT {
  id          String   @id @default(cuid()) @map("_id")
  name        String
  description String
  minted      Boolean  @default(false)
  image       String
  attributes  Json?   
  owner       String?
  qrPath      String?    
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
